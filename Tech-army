    <script>
       // JavaScript code for slideshow animation
var currentSlide = 0;

function changeSlide(n) {
    var slides = document.getElementsByClassName("slide");
    var dots = document.getElementsByClassName("nav-dot");

    // Hide all slides
    for (var i = 0; i < slides.length; i++) {
        slides[i].style.opacity = 0;
    }

    currentSlide += n;

    if (currentSlide >= slides.length) {
        currentSlide = 0;
    }

    if (currentSlide < 0) {
        currentSlide = slides.length - 1;
    }

    // Display the current slide
    slides[currentSlide].style.opacity = 1;

    // Update the navigation dots
    for (var i = 0; i < dots.length; i++) {
        dots[i].style.backgroundColor = "rgba(0, 0, 0, 0.6)";
    }
    dots[currentSlide].style.backgroundColor = "rgba(0, 0, 0, 0.8)";
}
</script>
